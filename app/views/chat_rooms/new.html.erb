<div class="col-md-4 col-md-offset-4">
	<h1>Add chat room</h1>

	<%= simple_form_for @chat_room do |f| %>
		<div class="form-group">
			<%= f.input :title, input_html: { class: 'form-control' } %>
		</div>

		<div class="form-actions">
			<%= f.submit 'Submit', class: 'btn btn-primary' %>
		</div>
	<% end %>
</div>

